@model IEnumerable<string>

<html>
    <head>
        <link href="~/Content/DigitalWorldLayoutStyle.css" rel="stylesheet" type="text/css" />
    </head>
    <body>
        @foreach (var link in Model)
        {

            @Html.RouteLink(link, new
   {
       controller = "Product",
       action = "List",
       category = link,
       page = 1
   }, new {@class = link == ViewBag.SelectedCategory ? "productCategorySelected" : "productCategory" })

        }
    </body>

</html>


